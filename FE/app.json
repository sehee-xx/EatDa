{
  "expo": {
    "name": "EatDa!",
    "slug": "eatda",
    "entryPoint": "index.js",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/logo.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/logo.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["assets/**/*"],
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "근처 가게를 찾기 위해 위치 정보가 필요합니다.",
        "NSLocationAlwaysAndWhenInUseUsageDescription": "근처 가게를 찾기 위해 위치 정보가 필요합니다."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/logo.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.sehee.eatda",
      "permissions": [
        "ACCESS_FINE_LOCATION",
        "ACCESS_COARSE_LOCATION"
      ],
      "softwareKeyboardLayoutMode": "resize"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-font", 
      "expo-video",
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "근처 가게를 찾기 위해 위치 정보가 필요합니다."
        }
      ]
    ],
    "extra": {
      "kakaoApiKey": "e7a0c8d5134218353ee9c9fa8a7b2634",
      "eas": {
        "projectId": "dbc7317c-c30e-4a26-97b6-18bea7e9e35c"
      }
    }
  }
}